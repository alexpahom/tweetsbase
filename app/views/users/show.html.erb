<h1>User: <%= @user.username %></h1>
<div>
  <table>
    <tr>
      <td>First name</td>
      <td><%= @user.first_name %></td>
    </tr>
    <tr>
      <td>Last name</td>
      <td><%= @user.last_name %></td>
    </tr>
    <tr>
      <td>Email address</td>
      <td><%= @user.email %></td>
    </tr>
    <tr>
      <td>Phone number</td>
      <td><%= @user.phone %></td>
    </tr>
  </table>
    <div>
<%= link_to 'Update', edit_user_path(@user) %>
<%= link_to 'Cancel', users_path %>
    </div>
</div>